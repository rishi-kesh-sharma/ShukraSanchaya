
<div style="overflow-x: auto;padding-top: 80px;">
    <table>
      <tr>
          <th>S.N</th>
          <th>Name</th>
          <th>Email</th>
          <th>Phone</th>
          <th>Address</th>
          <th>Edit</th>
          <th>Remove</th>
        </tr>
          
        <% hospitals.forEach((hospital,index) => { %>
          <tr>
           <% const {_id,name,email,phone,address}=hospital; %>
           <td  contenteditable="false"><%=skip+index+1%></td>
           <td class="hospital-data" contenteditable="true"><%=name%></td>
           <td class="hospital-data" contenteditable="true"><%=email%></td>
           <td class="hospital-data" contenteditable="true"><%=phone%></td>
           <td class="hospital-data" contenteditable="true"><%=address%></td>
           <td><i class="fa-solid fa-pen-to-square edit-btn" id=<%=_id%> ></i></td>
           <td><i class="fa fa-trash remove-btn" aria-hidden="true" id=<%=_id%> ></i></td>
          </tr>
         <% })%>
      </table>
  </div>
